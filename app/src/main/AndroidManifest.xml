        android:supportsRtl="true"
        android:theme="@style/Theme.Bank_sys">
        <activity android:name=".list_history"></activity>
        <activity android:name=".send_to"></activity>
        <activity android:name=".user_prof" />
        <activity android:name=".view_members" />
